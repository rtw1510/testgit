				<rule id="CtrctCertRels_CollectionRepeat" type="CollectionRepeat"
					errCode="ABE011" fbTag="BrerNm" fbValue="${CtrctCertRelSgmt.BrerNm}"
					depExp="CtrctCertRels_NotNull">
					<property name="repeat" value="false" />
					<property name="beanClass"
						value="org.pbccrc.archive.collect.messagetools.ntrcontract.message.segment.CtrctCertRel" />
					<property name="fieldNames" value="BrerType;CertRelIDType;CertRelIDNum" />
				</rule>
				<field name="CtrctCertRel_bean" fbTag="CtrctCertRel"
					classType="org.pbccrc.archive.collect.messagetools.ntrcontract.message.segment.CtrctCertRel">
					<rule id="CtrctCertRel_bean_CertRelIDNum_ntr_Vali" type="NtrCert"
						errCode="ABE004" fbTag="CertRelIDNum" fbValue="${$this.CertRelIDNum}"
						depExp="CtrctCertRel_bean-CertRelIDType_Ntr_dict">
						<property name="certTypeFeild" value="CertRelIDType"></property>
						<property name="certNumFeild" value="CertRelIDNum" />
					</rule>
					<rule id="CtrctCertRel_bean_CertRelIDNum_leg_Vali" type="LegCert"
						errCode="ABE004" fbTag="CertRelIDNum" fbValue="${$this.CertRelIDNum}"
						depExp="CtrctCertRel_bean-CertRelIDType_Org_dict">
						<property name="certTypeFeild" value="CertRelIDType"></property>
						<property name="certNumFeild" value="CertRelIDNum" />
					</rule>
					<rule id="CtrctCertRel_bean_co" fbTag="0000" fbValue="" type="Method"
						errCode="BCE003">
						<package>
							org.pbccrc.archive.collect.messagetools.ntrcontract.message.segment.CtrctBsSgmt;
							org.pbccrc.archive.collect.messagetools.ntrcontract.message.segment.CtrctCertRel;
							org.pbccrc.archive.collect.messagetools.ntrcontract.message.segment.CtrctCertRelSgmt;
							org.pbccrc.archive.collect.messagetools.validators.utils.ValueUtil;
						</package>
						<method>
							{
							CtrctBsSgmt ctrctBsSgmt = record.getCtrctBsSgmt();
							if
							(ValueUtil.hasNull(ctrctBsSgmt))
							return true;
							String cIdNum =
							ctrctBsSgmt.getIDNum();
							String cIdType = ctrctBsSgmt.getIDType();
							if (ValueUtil.hasNull(ctrctBsSgmt, cIdNum, cIdType)) {
							return true;
							}
							CtrctCertRelSgmt ccSgmt = record.getCtrctCertRelSgmt();
							if
							(ccSgmt == null || ccSgmt.getCtrctCertRels() == null ||
							ccSgmt.getCtrctCertRels().size()==0) {
							return true;
							}
							for(int i=0 ;i
							&lt; ccSgmt.getCtrctCertRels().size();i++){
							CtrctCertRel ccR =
							(CtrctCertRel)ccSgmt.getCtrctCertRels().get(i);
							if(ccR.getBrerType()==null){
							return true;
							}
							if(ccR.getBrerType().equals("1")){
							if(ValueUtil.hasNull(ccR.getCertRelIDType(),
							ccR.getCertRelIDNum())){
							return true;
							}
							if(ccR.getCertRelIDType().equals(cIdType) &amp;&amp;
							ccR.getCertRelIDNum().equals(cIdNum)){
							return false;
							}
							}
							}
							return
							true;
							}
						</method>
					</rule>

					<field name="BrerType" fbTag="BrerType" classType="java.lang.String">
